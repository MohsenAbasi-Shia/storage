			<node custom_icon_id="0" foreground="" is_bold="False" name="remote" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1598340670.26" ts_lastsave="1601362878.75" unique_id="37">
				<rich_text>Delete remote branch:
    $ git push origin --delete infra1-Dockerize</rich_text>
