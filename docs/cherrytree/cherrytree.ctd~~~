					<rich_text>apiVersion: v1
kind: Pod
metadata: 
  name: nginx 
  labels: 
    app: nginx 
spec: 
  containers: 
  - name: nginx
    image: nginx
    port:80
    env:
    - name: GREETING
      value: &quot;Warm greetings to&quot;
    - name: HONORIFIC
      value: &quot;The Most Honorable&quot;
    - name: NAME
      value: &quot;Kubernetes&quot;
