					<rich_text>#!/bin/bash 
 
# usage: ./minio-upload my-bucket my-file.zip 
 
bucket=$1 
file=$2 
 
host=file.razavi.ir 
s3_key='minio-file' 
s3_secret='jARRCuZQd4sm32t8THUEfLqADxot9ci4W3pki9F7' 
 
resource=&quot;/${bucket}/${file}&quot; 
content_type=&quot;application/octet-stream&quot; 
date=`date -R` 
_signature=&quot;PUT\n\n${content_type}\n${date}\n${resource}&quot; 
signature=`echo -en ${_signature} | openssl sha1 -hmac ${s3_secret} -binary | base64` 
 
curl -v -X PUT -T &quot;${file}&quot; \ 
          -H &quot;Host: $host&quot; \ 
          -H &quot;Date: ${date}&quot; \ 
          -H &quot;Content-Type: ${content_type}&quot; \ 
          -H &quot;Authorization: AWS4-HMAC-SHA256 ${s3_key}:${signature}&quot; \ 
          https://$host${resource}
 
</rich_text>
